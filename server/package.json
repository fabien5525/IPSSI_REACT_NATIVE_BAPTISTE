{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "type": "commonjs",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --ext ts --exec npm run dev-start",
    "dev-start": "npm run build && node dist/index.js",
    "build": "npm run clean && babel src --out-dir dist --extensions \".ts\" --copy-files",
    "clean": "node -e \"const fs = require('fs-extra'); if (fs.existsSync('dist')) fs.removeSync('dist')\"",
    "sequelize": "sequelize",
    "s": "sequelize",
    "s:m": "npm run sequelize -- db:migrate",
    "s:m:c": "npm run sequelize -- migration:create --name",
    "s:m:u": "npm run sequelize -- db:migrate:undo",
    "s:s": "npm run sequelize -- db:seed:all",
    "s:s:u": "npm run sequelize -- db:seed:undo:all"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.2.5",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "jsonwebtoken": "^9.0.0",
    "sequelize": "^6.31.1",
    "sequelize-cli": "^6.6.0",
    "sqlite3": "^5.1.6",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.2",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/node": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/plugin-transform-typescript": "^7.21.3",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@types/continuation-local-storage": "^3.2.4",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.13",
    "@types/node": "^18.15.11",
    "@types/nodemon": "^1.19.2",
    "nodemon": "^2.0.22"
  }
}
